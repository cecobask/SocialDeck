{
  "name": "socialdeck",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "engines": {
    "node": "10.16.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=dev node dist/server.js",
    "test": "cross-env NODE_ENV=test mocha --exit",
    "test:report": "cross-env NODE_ENV=test nyc mocha --exit",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "eslint \"**/*.js\" --fix",
    "clean": "rimraf ./dist && mkdir dist",
    "compile:models": "babel ./models --out-dir dist/models",
    "compile:routes": "babel ./routes --out-dir dist/routes",
    "compile:schema": "babel ./schema --out-dir dist/schema ",
    "compile:app": "babel app.js db.js server.js --out-dir dist/",
    "compile:copy": "cp -r node_modules/ dist/node_modules && cp -r views/ dist/views && cp -r public/ dist/public && cp -r secrets/ dist/secrets",
    "compile:all": "npm-run-all compile:models compile:routes compile:schema compile:app compile:copy",
    "prebuild": "npm-run-all test clean lint",
    "build:release": "npm-run-all test clean lint compile:all",
    "postbuild": "npm start",
    "postinstall": "rimraf ./dist && npm run compile:all"
  },
  "author": "Tsvetoslav Dimov",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.0.0-beta.55",
    "apollo-server": "^2.9.6",
    "apollo-server-express": "^2.9.6",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "connect-mongo": "^3.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.1",
    "eslint": "^6.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-scalars": "^1.0.2",
    "graphql-tools": "^4.0.5",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mocha": "^6.2.2",
    "moment": "^2.24.0",
    "mongodb-memory-server": "^6.0.1",
    "mongoose": "^5.7.5",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
